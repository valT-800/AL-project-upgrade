{
  "name": "project-upgrade",
  "publisher": "Simplanova",
  "displayName": "Project Upgrade Preparation",
  "icon": "./simplanovalogo.jpg",
  "description": "Project Upgrade Preparation tool will help you to make all needed project preparations to start project upgrade to extensions",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.addApplicationArea",
        "title": "SPLN: Add Application Area in active project"
      },
      {
        "command": "extension.projectPrep",
        "title": "SPLN: Prepare project for upgrade"
      },
      {
        "command": "extension.addSuffix1",
        "title": "SPLN: Add Suffix step 1"
      },
      {
        "command": "extension.addSuffix2",
        "title": "SPLN: Add Suffix step 2. Resolve errors"
      },
      {
        "command": "extension.addSuffix3",
        "title": "SPLN: Add Suffix step 3. Clean up!"
      },
      {
        "command": "extension.addPrefix1",
        "title": "SPLN: Add Prefix step 1"
      },
      {
        "command": "extension.addPrefix2",
        "title": "SPLN: Add Prefix step 2. Resolve errors"
      },
      {
        "command": "extension.addPrefix3",
        "title": "SPLN: Add Prefix step 3. Clean up!"
      },
      {
        "command": "extension.generalRefactoring",
        "title": "SPLN: General Refactoring in active project"
      },
      {
        "command": "extension.generalActiveRefactoring",
        "title": "SPLN: General Refactoring in active files"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valT-800/AL-project-upgrade"
  }
}